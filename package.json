{
  "name": "wemlion-nuxt-helper",
  "displayName": "wemlion-nuxt-helper",
  "description": "Umi locale helper",
  "version": "0.0.1",
  "publisher": "wemlion",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
