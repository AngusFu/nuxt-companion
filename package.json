    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@oxc-project/types": "^0.56.5",
    "esquery": "^1.6.0",
    "lodash": "^4.17.21",
    "oxc-parser": "^0.56.5"
