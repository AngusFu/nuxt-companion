{
  "name": "wemlion-nuxt-helper",
  "displayName": "Nuxt Companion",
  "version": "0.1.0",
  "description": "nuxt helper",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AngusFu/nuxt-companion"
  },
  "publisher": "wemlion",
  "main": "./out/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "vscode:prepublish": "make build"
  },
  "contributes": {
    "snippets": [],
    "commands": [
      {
        "command": "nuxtCompanion.toggle",
        "title": "Toggle Nuxt Companion"
      }
    ],
    "configuration": {
      "title": "Nuxt Companion Settings",
      "properties": {
        "nuxtCompanion.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable Nuxt Companion"
        },
        "nuxtCompanion.enableLayoutsNameIntelligence": {
          "type": "boolean",
          "default": true,
          "description": "Enable layouts name intelligence"
        },
        "nuxtCompanion.enableTypedRoutesIntelligence": {
          "type": "boolean",
          "default": true,
          "description": "Enable typed routes intelligence"
        },
        "nuxtCompanion.enableApiToServer": {
          "type": "boolean",
          "default": true,
          "description": "Enable api to server"
        },
        "nuxtCompanion.enableGoToAlias": {
          "type": "boolean",
          "default": true,
          "description": "Enable go to alias"
        }
      }
    }
  },
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@oxc-parser/wasm": "^0.60.0",
    "@oxc-project/types": "^0.62.0",
    "@types/esquery": "^1.5.4",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.13.0",
    "esquery": "^1.6.0",
    "globby": "^14.1.0",
    "jest": "^29.7.0",
    "lodash-es": "^4.17.21",
    "pathe": "^2.0.3",
    "scule": "^1.3.0",
    "ts-jest": "^29.3.1",
    "typescript": "^4.7.2",
    "ufo": "^1.5.4"
  },
  "engines": {
    "vscode": "^1.34.0"
  },
  "icon": "assets/icon.png"
}
